<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
	android:key="@string/settingsKey"
	android:persistent="false" >
	<PreferenceCategory
		android:title="@string/widgetListCategory">
		<autonag.CustomPreference
			android:key="@string/widgetCurrentListKey"
	        android:title="" />
	</PreferenceCategory>
	<PreferenceCategory
		android:title="@string/widgetSelectListCategory"
		android:key="@string/widgetSelectListCategoryKey" />
	<PreferenceCategory
		android:title="@string/listManagementCategory" >
		<autonag.CustomPreference
			android:key="@string/createListKey"
	        android:title="@string/createListTitle" />
	</PreferenceCategory>
	<PreferenceCategory
		android:title="@string/helpAboutCategory" >
	    <Preference
	        android:title="@string/helpSettingsTitle"
	        android:summary="@string/helpSettingsSummary" >
	        <intent
	        	android:targetPackage="AutoNag.AutoNag"
	            android:targetClass="autonag.HelpDialogueActivity" />
	    </Preference>
	</PreferenceCategory>
</PreferenceScreen>