<?xml version="1.0" encoding="UTF-8" ?>
<!--
Draw an orange vertical line on the LHS. Background white
-->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
	<item>
		<shape android:shape="rectangle" > 
			<solid android:color="@color/itemNormalBackground"/>    
			<stroke
				android:width="4dp"
				android:color="@color/itemLeftBorder" />
		</shape>
	</item>
	<item android:left="4dp" >
		<shape android:shape="rectangle" > 
			<solid android:color="@color/itemNormalBackground"/>    
		</shape>
	</item>
</layer-list>